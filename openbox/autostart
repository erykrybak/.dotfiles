# If you want to use XFCE config tools...
#
#xfce-mcs-manager &
tint2 &
picom -b &
hsetroot -solid "#1E201E" &
xfce4-power-manager &
dunst -config ~/.config/dunst/dunstrc &
greenclip daemon &
(sleep 0.5; volumeicon) &
nm-applet &
xinput set-prop 10 "libinput Tapping Enabled" 1
xinput set-prop 10 "libinput Accel Speed" 0.3
